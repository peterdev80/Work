                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill)" Storyboard.TargetName="V_1" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource W}"/>
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.3" Value="{StaticResource Y}"/>
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.6" Value="{StaticResource W}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill)" Storyboard.TargetName="V_2" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource Y}"/>
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.3" Value="{StaticResource W}"/>
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.6" Value="{StaticResource Y}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="ost"/>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="State" Value="2">
                            <Setter TargetName="V_1" Property="Fill" Value="Red"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
