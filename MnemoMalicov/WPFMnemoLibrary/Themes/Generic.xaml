    <Style TargetType="{x:Type local:KeyTypeButtonLeft}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:KeyTypeButtonLeft}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="3"/>
                            <RowDefinition Height="0.700*"/>
                            <RowDefinition Height="3"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="3"/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="brdLeft" Grid.Row="0" Grid.RowSpan="3" Background="{TemplateBinding Background}" BorderBrush="#FFF71313" Visibility="Hidden"/>
                        <Border x:Name="brdReiht" Grid.Row="0" Grid.RowSpan="3" Background="{TemplateBinding Background}" BorderBrush="#FFF71313" Grid.Column="2" Visibility="Hidden"/>
                        <Border x:Name="brdUP" Grid.Row="0" Grid.RowSpan="1" Background="{TemplateBinding Background}" BorderBrush="#FFF71313" Grid.ColumnSpan="3" Visibility="Hidden"/>
                        <Border x:Name="brdDown" Grid.Row="2" Grid.RowSpan="1" Background="{TemplateBinding Background}" BorderBrush="#FFF71313" Grid.ColumnSpan="3" Visibility="Hidden"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <!--Left open-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="False"/>
                                <Condition Property="KeyType" Value="Left"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdLeft" Value="Hidden"/>
                            <Setter Property="Visibility" TargetName="brdReiht" Value="Visible"/>
                        </MultiTrigger>
                        <!--Left Close-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="True"/>
                                <Condition Property="KeyType" Value="Left"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdLeft" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="brdReiht" Value="Hidden"/>
                        </MultiTrigger>
                        <!--R open-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="True"/>
                                <Condition Property="KeyType" Value="Reiht"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdLeft" Value="Hidden"/>
                            <Setter Property="Visibility" TargetName="brdReiht" Value="Visible"/>
                        </MultiTrigger>
                        <!--R close-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="False"/>
                                <Condition Property="KeyType" Value="Reiht"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdLeft" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="brdReiht" Value="Hidden"/>
                        </MultiTrigger>

                        <!--U open-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="False"/>
                                <Condition Property="KeyType" Value="Up"/>
                            </MultiTrigger.Conditions>

                            <Setter Property="Visibility" TargetName="brdDown" Value="Visible"/>
                        </MultiTrigger>
                        <!--U close-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="True"/>
                                <Condition Property="KeyType" Value="Up"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdUP" Value="Visible"/>

                        </MultiTrigger>

                        <!--D open-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="False"/>
                                <Condition Property="KeyType" Value="Down"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdUP" Value="Visible"/>
                        </MultiTrigger>
                        <!--D close-->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="KeyState" Value="True"/>
                                <Condition Property="KeyType" Value="Down"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="brdDown" Value="Visible"/>
                        </MultiTrigger>

                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


